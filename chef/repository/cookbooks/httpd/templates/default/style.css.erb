body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,textarea,p,blockquote,th,td { 
	margin:0;
	padding:0;
}
body {
  font: 62.5%/1em Helvetica, sans-serif;
  }
table {
	border-collapse:collapse;
	border-spacing:0;
}
fieldset,img { 
	border:0;
}
address,caption,cite,code,dfn,em,/*strong*/,th,var {
	font-style:normal;
	font-weight:normal;
}
ol,ul {
	list-style:none;
}
caption,th {
	text-align:left;
}
h1,h2,h3,h4,h5,h6 {
	font-size:100%;
	font-weight:normal;
}
q:before,q:after {
	content:'';
}
abbr,acronym { border:0;
}
html {	
	background: #fff url('images/body-back.png') repeat-x;;
	}
body {
	padding: 0 45px 0;
	margin: 0 auto;	
	width: 935px;
    position: static!important;
	}
a {
  text-decoration: none;
  }
html {
  height: 100%;
  background: #fff url('images/body-back.png') repeat-x;
    }

/* id="maintenance-medal" */

#maintenance-medal {
  width: 755px;
}
#maintenance-medal .body {
  margin-top: 6px;
  width: 330px;
  padding: 195px 55px 240px 370px;
  background: url(images/maintenance-medal.png) no-repeat 100px 0;
  color: #a4a4a4;
}
#maintenance-medal .body span {
  display: block;
}
#maintenance-medal .body  .sp1 {
  font: bold 27px/25px helveticaneue, sans-serif;
  text-transform: uppercase; 
}
#maintenance-medal .body  .sp2 {
  font: bold 43px/40px helveticaneue, sans-serif;
  text-transform: uppercase;  
}
#maintenance-medal .body  .sp3 {
  font: 17px/17px helvetica, sans-serif; 
  word-spacing: -1px;
  margin-bottom: 35px;
  text-indent: 2px;
}
#maintenance-medal .body  .sp4 {
  font: bold 18px/18px helvetica, sans-serif;  
  text-transform: uppercase;
  margin-bottom: 6px;
  text-indent: 1px;
}
#maintenance-medal .body  .sp5 {
  font: 14px/14px helvetica, sans-serif;  
  word-spacing: -2px;
  text-indent: 1px;
}
#maintenance-medal .body  .sp5 a {
  font-weight: bold; 
  text-decoration: none;
  color: #a4a4a4;
}
#maintenance-medal .body .sp5 a:hover {
  color: red;
}

/* id="maintenance-umbrella" */

#maintenance-umbrella .body {
  margin-top: 6px;
  width: 330px;
  padding: 215px 210px 240px 460px;
  background: url(images/maintenance-umbrella.png) no-repeat 95px 0;
  color: #888888;
}
#maintenance-umbrella .body span {
  display: block;
}
#maintenance-umbrella .body  .sp1 {
  font: bold 70px/70px helveticaneue, sans-serif;
  text-transform: uppercase; 
  color: #999999;
}
#maintenance-umbrella .body  .sp2 {
  font: bold 13px/16px helveticaneue, sans-serif;
  text-transform: uppercase;  
  text-indent: 4px;
}
#maintenance-umbrella .body  .sp3 {
  font: italic 13px/16px helveticaneue, sans-serif;  
  text-transform: uppercase; 
  text-indent: 4px;
}
